<h1>ユーザー 一覧</h1>
<% @messages.each do |message| %>
<p>
<%= message.name %>
<%= link_to "詳細", message_path(message.id) %>
<%= link_to "編集", edit_message_path(message.id) %>
<%= link_to "削除", message_path(message.id), method: :delete, data: {confirm: "削除しますか？"} %>
</p>
<% end %>
<%= link_to "投稿画面へ", new_message_path %>